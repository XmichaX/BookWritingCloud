<div class="row">




  <div class="col-lg-10 col-sm-11 responsive">

    <%= render 'books/filterbar' %>

    <hr class="featurette-divider">
    ola
    <%= @i %>
    <%= @users %>
    <br>
    <table class="table table-striped books-content">
      <thead>
      <tr>
        <th><%= Book.human_attribute_name(:title) %></th>
        <th><%= Book.human_attribute_name(:edition) %></th>
        <th><%= Book.human_attribute_name(:published) %></th>
        <th><%= Book.human_attribute_name(:genre) %></th>
        <th><%= Book.human_attribute_name(:tags) %></th>
        <th><%= Book.human_attribute_name(:users) %></th>
      </tr>
      </thead>
      <tbody>
      <% @books.each do |book| %>
          <tr>

            <td><%= link_to book.title, book %></td>
            <td><%= book.edition %></td>
            <td><%= book.published %></td>
            <td><%= book.genre %></td>
            <td><%= book.tags %></td>
            <td><%= book.users_list %></td>
          </tr>

      <% end %>
      </tbody>
    </table>
  </div>
</div>

<%= render 'overlayNewBook' %>
